<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Aden</title>


    <link href="~/src/scss/bootstrap/bootstrap.css" rel="stylesheet" />
    <link href="~/src/scss/fontawesome/fontawesome.css" rel="stylesheet" />
    <!-- DevExtreme themes -->
    <link href="~/css/dx-bundle.css" rel="stylesheet" />

    <link href="~/src/scss/bootstrap-dialog/bootstrap-dialog.css" rel="stylesheet" />
    <link href="~/src/scss/main/main.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")

    <style>
    
    </style>
</head>
<body>

    @Html.Partial("_StatusHeader")
    @Html.Partial("_Header")
    @Html.Partial("_NavBar")

    <div class="container-fluid" id="content">
        <div class="page" id="page">
            @RenderBody()
        </div>
    </div>

    @Html.Partial("_Footer")


    <script src="~/node_modules/jquery/dist/jquery.js"></script>
    <script src="~/node_modules/bootstrap-sass/assets/javascripts/bootstrap.js"></script>

    <!-- DevExtreme library -->
    <script src="~/js/dx-bundle.js"></script>

    <script src="~/js/jquery.serializejson.min.js"></script>

    <script src="~/node_modules/bootstrap3-dialog/dist/js/bootstrap-dialog.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
